<Window x:Class="DataReader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="700" Width="1000">
    <Grid>
        <Image Name="Color" Margin="10,10,639,418"/>
        <TextBox x:Name="FrameInputField" Height="20" Width="50"/>
        <Button x:Name="FrameInputBtn" Height="20" Width="50" Margin="521,324,421,325" Click="FrameInput_Click">Input</Button>
        <StackPanel Height="30" Width="300" Margin="28,256,664,383">
            <Slider Name="FrameController" ValueChanged="FrameController_Changed" Maximum="99" TickPlacement="BottomRight" TickFrequency="1" IsSnapToTickEnabled ="true" />
        </StackPanel>

    </Grid>
</Window>
